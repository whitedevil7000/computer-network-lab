   "source": [
    "import socket\n",
    "HOST = '127.0.0.1'\n",
    "PORT = 9999\n",
    "s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "s.connect((HOST, PORT))\n",
    "str ='Hello, world'\n",
    "b = str.encode('utf-8')\n",
    "s.send(b)\n",
    "data = s.recv(1024)\n",
    "s.close()\n",
    "print ('Received', data)\n"
   ]
